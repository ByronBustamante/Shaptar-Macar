<p id="notice"><%= notice %></p>

<h1>Listing Suppliers</h1>

<table>
  <thead>
    <tr>
      <th>Supplier code</th>
      <th>Nit</th>
      <th>Supplier name</th>
      <th>Company name</th>
      <th>Country</th>
      <th>Department or state</th>
      <th>Full address</th>
      <th>Telephone 1</th>
      <th>Telephone 2</th>
      <th>Mobile 1</th>
      <th>Mobile 2</th>
      <th>Email</th>
      <th>Web page</th>
      <th>Observations</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @suppliers.each do |supplier| %>
      <tr>
        <td><%= supplier.supplier_code %></td>
        <td><%= supplier.nit %></td>
        <td><%= supplier.supplier_name %></td>
        <td><%= supplier.company_name %></td>
        <td><%= supplier.country %></td>
        <td><%= supplier.department_or_state %></td>
        <td><%= supplier.full_address %></td>
        <td><%= supplier.telephone_1 %></td>
        <td><%= supplier.telephone_2 %></td>
        <td><%= supplier.mobile_1 %></td>
        <td><%= supplier.mobile_2 %></td>
        <td><%= supplier.email %></td>
        <td><%= supplier.web_page %></td>
        <td><%= supplier.observations %></td>
        <td><%= link_to 'Show', supplier %></td>
        <td><%= link_to 'Edit', edit_supplier_path(supplier) %></td>
        <td><%= link_to 'Destroy', supplier, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Supplier', new_supplier_path %>
