<p id="notice"><%= notice %></p>

<h1>Listing Location In Warehouses</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @location_in_warehouses.each do |location_in_warehouse| %>
      <tr>
        <td><%= location_in_warehouse.location %></td>
        <td><%= link_to 'Show', location_in_warehouse %></td>
        <td><%= link_to 'Edit', edit_location_in_warehouse_path(location_in_warehouse) %></td>
        <td><%= link_to 'Destroy', location_in_warehouse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location in warehouse', new_location_in_warehouse_path %>
