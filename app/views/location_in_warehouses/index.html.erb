<div class="row">
  <div class="col-xs-12">
    <h1>Ingresar Ubicación en Bodega</h1>
  </div>
</div>

<%= render 'form' %>

<div class="row">
  <div class="col-xs-12">
    <table>
      <thead>
        <tr>
          <th>Ubicación</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @location_in_warehouses.each do |location_in_warehouse| %>
          <tr>
            <td><%= location_in_warehouse.location %></td>
            <td class="text-end"><%= link_to 'Editar', edit_location_in_warehouse_path(location_in_warehouse) %> | <%= link_to 'Eliminar', location_in_warehouse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
